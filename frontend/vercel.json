{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/trpc/(.*)",
      "destination": "https://api.sniptxt.xyz/trpc/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://api.sniptxt.xyz/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
